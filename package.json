    "@types/elliptic": "6.4.18",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/lodash": "4.17.20",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
