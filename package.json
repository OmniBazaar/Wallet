    "lodash": "^4.17.0",
    "multiformats": "^12.1.0",
    "nanoid": "^5.0.0",
    "omnibazaar-validator": "file:../Validator",
    "orbit-db": "^0.29.0",
    "pinia": "^2.1.0",
    "secp256k1": "^5.0.0",
    "styled-components": "^6.1.19",
    "tiny-secp256k1": "^2.2.4",
    "tweetnacl": "^1.0.3",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "web3": "^4.2.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.0",
